{
  "root": true,
  "plugins": ["no-loops", "prettier"],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "extends": ["eslint:recommended", "prettier"],
  "env": {
    "es2021": true,
    "node": true
  },
  "rules": {
    "no-console": 1, // Means warning
    "prettier/prettier": 2 // Means error
  }
}
